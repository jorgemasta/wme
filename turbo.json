{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "dist/**"
      ]
    },
    "dev": {
		"cache": false
	},
    "lint": {
      "outputs": []
    },
    "lint:fix": {
		"cache": false
	},
	"watch": {
		"cache": false
	}
  }
}
